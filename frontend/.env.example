# SmartFactory CONNECT - Frontend Environment Variables
# Copy this file to .env and update values accordingly

# API Configuration
# For local development:
# VITE_API_URL=http://localhost:3000/api
# VITE_WS_URL=http://localhost:3000

# For production (Cloudflare Tunnel):
VITE_API_URL=https://api.xiao.software/api
VITE_WS_URL=https://api.xiao.software

# RAG Service
# For local: http://localhost:8001
# For production:
VITE_RAG_API_URL=https://api.xiao.software/rag

# App Configuration
VITE_APP_NAME=SmartFactory CONNECT
VITE_APP_VERSION=2.0.0

# Environment (development | production)
VITE_NODE_ENV=production

# Optional: Google Analytics, Sentry, etc.
# VITE_GA_ID=
# VITE_SENTRY_DSN=
